# Cloudflare Worker 配置
WORKER_URL=https://your-worker.workers.dev
API_KEY=your-api-key-here

# 代理配置（可选）
USE_PROXY=false
PROXY_PORT=7897

# 爬取配置
SLEEP_TIME=120              # 每次爬取间隔（秒）
DAILY_REQUEST_LIMIT=500     # 每日请求限制（避免触发 Cloudflare 限制）
AUTO_CONTINUE=false         # 是否自动跨天继续（true=自动，false=手动）

# 预测策略配置
# 可用策略: frequency, random, balanced, coldHot
# 多个策略用逗号分隔，例如: frequency,balanced,coldHot
DEFAULT_STRATEGIES=frequency,random,balanced,coldHot

# 默认预测条数
# 建议：使用策略数的倍数，确保每个策略均匀分配
# 例如：4个策略建议使用 4, 8, 12 等
DEFAULT_PREDICTION_COUNT=4
